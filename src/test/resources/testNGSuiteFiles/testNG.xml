<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ChristoTest" thread-count="3" parallel="methods" data-provider-thread-count="2">
    <listeners>
        <listener class-name="org.ctf.listeners.ChristoListener"></listener>
        <listener class-name="org.ctf.listeners.MethodInterceptor"></listener>
        <listener class-name="org.ctf.listeners.AnnotationTransformer"></listener>
    </listeners>
    <test name="Regression">
        <classes>
            <class name="org.ctf.testcases.orangeHRM.LoginPageTest"/>
            <class name="org.ctf.testcases.orangeHRM.AdminPageTest"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->